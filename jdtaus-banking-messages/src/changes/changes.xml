  <body>
    <!-- *********************************************************************************************************** -->
    <release version="1.0" date="2007-08-01" description="First stable release."/>
    <!-- *********************************************************************************************************** -->
    <release version="1.1" date="2007-08-24" description="Feature enhancements.">
      <action dev="schulte2005" type="update"><![CDATA[
        Added class <code>org.jdtaus.banking.messages.ReadsBankleitzahlenDateiMessage</code>.]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.2" date="2007-12-23" description="Feature enhancements.">
      <action dev="schulte2005" type="update"><![CDATA[
        Added classes<ul>
        <li><code>org.jdtaus.banking.messages.UnknownBankleitzahlMessage</code></li>
        <li><code>org.jdtaus.banking.messages.BankleitzahlExpirationMessage</code></li>
        <li><code>org.jdtaus.banking.messages.BankleitzahlReplacementMessage</code></li>
        </ul>]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.2.1" date="2007-12-25" description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
        Updated classes<ul>
        <li><code>org.jdtaus.banking.messages.UnknownBankleitzahlMessage</code>
        <blockquote>to correctly format message text.</blockquote></li></ul>]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.2.2" date="2008-03-13" description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
        Added various missing serial version UID fields. Updated to latest
        mojo releases.]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.2.3" date="2008-07-06" description="Maintenance release."/>
    <!-- *********************************************************************************************************** -->
    <release version="1.3" date="2008-10-28" description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Upgraded to latest core features.<ul>
      <li>Removed any internal properties resources and moved any messages to
      the META-INF/jdtaus/module.xml module descriptor.</li></ul>]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.8" date="2008-12-29" description="Maintenance release.">
      <action dev="schulte2005" type="fix"><![CDATA[
      Upgraded to jDTAUS Core version 1.8 to support exchanging references to
      the <code>java.util.Locale</code> implementation to use.]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.8.1" date="2009-05-19" description="Maintenance release.">
      <action dev="schulte2005" type="fix"><![CDATA[
      Corrected a format argument of the message of class
      <code>org.jdtaus.banking.messages.ChecksumErrorMessage</code>.]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.9" date="2009-07-02" description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Updated any references to the jDTAUS server to point to the SF.net
      servers.]]>
      </action>
      <action dev="schulte2005" type="fix">Corrected GPG signatures.</action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.10" date="2009-07-14" description="Maintenance release."/>
    <!-- *********************************************************************************************************** -->
    <release version="1.11" date="2009-12-05" description="Feature enhancements.">
      <action dev="schulte2005" type="update"><![CDATA[
      Added classes<ul>
      <li>org.jdtaus.banking.messages.ReadsCurrenciesMessage</li>
      <li>org.jdtaus.banking.messages.ReadsTextschluesselMessage</li>
      <li>org.jdtaus.banking.messages.SearchesBankleitzahlInfosMessage</li>
      <li>org.jdtaus.banking.messages.SearchesTextschluesselMessage</li>
      <li>org.jdtaus.banking.messages.SearchesCurrenciesMessage</li>
      </ul>]]>
      </action>
    </release>
    <!-- *********************************************************************************************************** -->
    <release version="1.11.1" date="2010-02-26" description="Maintenance release."/>
    <!-- *********************************************************************************************************** -->
